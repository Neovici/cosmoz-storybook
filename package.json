{
  "name": "cosmoz-storybook",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A storybook composition that allows you to browse cosmoz components",
  "scripts": {
    "lint": "eslint --cache --ext .js,.ts .",
    "start": "npm run storybook:start",
    "storybook:start": "storybook dev -p 8000",
    "storybook:build": "storybook build",
    "storybook:deploy": "storybook-to-ghpages",
    "storybook:preview": "npm run storybook:build && web-dev-server --root-dir ./storybook-static/ --open",
    "build": "tsc"
  },
  "devDependencies": {
    "@neovici/cfg": "^2.6.1",
    "@storybook/web-components-vite": "^9.1.7",
    "eslint": "^9.36.0",
    "http-server": "^14.1.1",
    "prettier": "^3.6.2",
    "storybook": "^9.1.7",
    "typescript": "~5.8.3",
    "vite": "^7.1.7"
  }
}
